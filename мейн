void printArr(T* a, int n) {
	for (int i = 0; i < n; i++)
		cout << a[i] << " "; 
}
int main()
{
	Myarr<AEROFLOT> arr;
	ifstream in("text1.txt");
	if (!in)cout << "\n file not open \n";
	in >> arr;
	cout << arr;
	insert(arr, 7);
	printArr(arr, 7);

